{
  "manifest_version": 2,

  "name": "SerialFiller",
  "description": "Wygeneruj losowe numery PESEL.",
  "version": "1.0",

  "background": { "scripts": ["serial-filler.js"]},

  "content_scripts": [{ 
    "matches": [ "<all_urls>"],
    "js": ["serial-filler-receiver.js"]
  }],

  "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": ["contextMenus"]
}